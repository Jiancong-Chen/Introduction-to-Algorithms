void insertionSort(int* a, int size){
	for (int i = 1; i < size; i++){
		int key = a[i];
		int j = i-1;

		while(j>=0&&a[j]<key){
			a[j+1] = a[j];
			j = j-1;
		}
		a[j+1] = key;
		for (int i = 0; i<size; i++){
			cout<<a[i]<<" ";
		}
		cout<<endl;
	}
	cout<<endl;
	for (int i = 0; i<size; i++){
		cout<<a[i]<<" ";
	}
	cout<<endl;
}
